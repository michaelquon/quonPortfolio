  @model Contact.ViewModel.ContactViewModel

    <section id="contact">
      <div class="container">
        <div class="row">
          <div class="col-lg-12 col-md-12 col-sm-12">
            <!-- START CONTACT HEADING -->
            <div class="heading">
              <h2 class="wow fadeInLeftBig">Contact Me</h2>
              <p>I am always looking to get involved with new projects. I would love to hear from you.</p>
            </div>
          </div>
        </div>       
        <div class="row">
          <!-- BEGIN CONTACT CONTENT -->
          <div class="contact_content">
            <!-- BEGIN CONTACT FORM -->
            <div class="col-lg-5 col-md-5 col-sm-5">
              <div class="contact_form">

                <!-- FOR CONTACT FORM MESSAGE -->
                <div id="form-messages"></div>
                @if (ViewBag.Message == null)
              {
                <form asp-controller="Home" method="post" asp-action="Index" id="cform">
                  <span asp-validation-for="Name" class="form-Error"></span>
                  <input asp-for="Name" class="form-control" type="text" placeholder="Your Name">
                  <span asp-validation-for="Email" class="form-Error"></span>
                  <input asp-for="Email" class="form-control" type="email" placeholder="Your Email Address">
                  <span asp-validation-for="Subject" class="form-Error"></span>
                  <input asp-for="Subject" class="form-control" type="text" placeholder="Subject">
                  <span asp-validation-for="Message" class="form-Error"></span>
                  <textarea asp-for="Message" class="form-control" cols="30" rows="10" placeholder="Your Message"></textarea>
                   <input class="submit_btn" type="submit" value="Send">  
                </form>
              }
               @if (ViewBag.Message != null)
            {
                   <h3 class="contact-reply">@ViewBag.Message</h3> 
                }
              </div>
              
            </div>
            <!-- BEGIN CONTACT MAP -->
            <div class="col-lg-7 col-md-7 col-sm-7">
              <div class="contact_map">
              <!-- BEGIN GOOGLE MAP -->
               <div id="map_canvas"></div>
              </div>
            </div>           
          </div>
        </div>      
      </div>             
    </section>
    <!--=========== END CONTACT SECTION ================-->

   

 
